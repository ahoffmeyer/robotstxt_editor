    "type": "typo3-cms-extension",
    "license": "GPL-2.0+",
    "version": "0.1.0",
    "keywords": ["TYPO3", "Extension", "SEO"],
    "authors": [
        {
            "name": "Andreas Hoffmeyer",
            "email": "hallo@andreas-hoffmeyer.de"
        }
